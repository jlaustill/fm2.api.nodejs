{
  "options": {
    "fileRoot": "./../filemanager",
    "uploadPath": "../uploads"
  },
  "security": {
    "capabilities": [ "select", "upload", "download", "rename", "move", "delete", "replace" ],
    "allowedFileTypes": [
      "jpg",
      "jpe",
      "jpeg",
      "gif",
      "png",
      "svg",
      "txt",
      "pdf",
      "odp",
      "ods",
      "odt",
      "rtf",
      "doc",
      "docx",
      "xls",
      "xlsx",
      "ppt",
      "pptx",
      "csv",
      "ogv",
      "avi",
      "mkv",
      "mp4",
      "webm",
      "m4v",
      "ogg",
      "mp3",
      "wav",
      "zip",
      "rar"
    ]
  }
}